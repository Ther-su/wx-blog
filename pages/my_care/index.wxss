page {
  background-color: #dedede;
}
navigator {
  height: 180rpx;
  background-color: #fff;
  display: flex;
  padding: 30rpx 30rpx;
  align-items: center;
  margin-bottom: 10rpx;
}
.image_wrap {
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
}
image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.name_wrap {
  font-size: 40rpx;
}
.first_row {
  display: flex;
  align-items: center;
  width: 200rpx;
  justify-content: space-around;
}
.second_row {
  display: flex;
  width: 300rpx;
  justify-content: space-around;
}
.info_wrap {
  flex: 5;
}